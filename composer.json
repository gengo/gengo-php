{
    "name": "gengo/gengo",
    "type": "library",
    "description": "Gengo API client library for PHP",
    "keywords": ["gengo", "translation", "api"],
    "homepage": "http://github.com/gengo/gengo-php",
    "license": "BSD",
    "authors": [
        {
            "name": "Vladimir Bashkirtsev",
            "email": "vladimir@bashkirtsev.com",
            "role": "Developer"
        },
        {
            "name": "Gengo",
            "email": "api@gengo.com",
            "homepage": "http://gengo.com",
            "role": "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Gengo\\": [ "src" ]
        },
        "files": [ "src/exceptions.php" ]
    },
    "require": {
        "php": ">=5.3.0",
        "ext-gettext": "*",
        "ext-json": "*",
        "logics/http": "0.1"
    },
    "require-dev": {
        "ext-SimpleXML": "*",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "phploc/phploc": "^2.1",
        "sebastian/phpcpd": "^2.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mamuz/php-dependency-analysis": "^0.6",
        "phpunit/phpunit": "^4.8",
        "logics/codesniffer": "0.1",
        "logics/configgenerator": "0.1"
    }
}